{
    "cleanOutputDir": true,
    "customFooterHtml": "<a href=\"https://nicolasventura.com/\" target=\"_blank\">Nicolas Ventura</a> &copy; 2024",
    "entryPointStrategy": "packages",
    "entryPoints": [
        "packages/*",
    ],
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "favicon": "icon.svg",
    "hideGenerator": true,
    "name": "npm.nicfv.com",
    "navigationLinks": {
        "Donate": "https://paypal.me/nicfv",
        "GitHub": "https://github.com/nicfv/npm",
        "npm": "https://www.npmjs.com/~nicfv",
        "YouTube": "https://www.youtube.com/@nikfv",
    },
    "packageOptions": {
        "alwaysCreateEntryPointModule": false,
        "disableSources": true,
        "includeVersion": true,
        "entryPoints": [
            "src/index.ts",
        ],
        "projectDocuments": [
            "CHANGELOG.md",
            "Example/*.md",
        ],
    },
    // "treatValidationWarningsAsErrors": true,
}