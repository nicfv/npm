{
  "name": "exray",
  "version": "1.0.4",
  "description": "Lightweight assertion testing framework",
  "homepage": "https://npm.nicfv.com/exray",
  "bin": "",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rm -rf dist types docs && typedoc --options ../typedoc.json src && node dist/test.js && rm dist/test.js types/test.d.ts",
    "test": "tsc -v && tsc --noEmit",
    "clean": "rm -rf node_modules package-lock.json dist types docs",
    "prepack": "npm run build",
    "postpack": "npm run clean"
  },
  "keywords": [
    "except",
    "exception",
    "test",
    "testing",
    "framework",
    "assert",
    "assertion",
    "pass",
    "fail"
  ],
  "author": {
    "name": "Nicolas Ventura",
    "email": "npm@nicfv.com",
    "url": "https://nicfv.com/"
  },
  "funding": {
    "type": "paypal",
    "url": "https://www.paypal.com/donate/?business=UM6EEKPW8GXA2&no_recurring=0&item_name=Open+source+development&currency_code=USD"
  },
  "repository": "github:nicfv/npm",
  "license": "MIT",
  "devDependencies": {
    "typedoc": "0.25.13"
  }
}