{
  "name": "viridis",
  "version": "1.0.4",
  "description": "Generate data visualization color gradients",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --removeComments",
    "types": "rm -rf types && tsc --declaration --declarationDir types --emitDeclarationOnly",
    "test": "tsc --noEmit",
    "clean": "rm -rf node_modules package-lock.json dist types docs",
    "docs": "rm -rf docs && typedoc --includeVersion --disableSources --hideGenerator src",
    "prepublishOnly": "npm run build && npm run types"
  },
  "keywords": [
    "color",
    "gradient",
    "palette",
    "viridis",
    "spectral",
    "parula",
    "chroma",
    "data",
    "viz",
    "visualization",
    "style"
  ],
  "author": {
    "name": "Nicolas Ventura",
    "email": "npm@nicolasventura.com",
    "url": "https://nicolasventura.com/"
  },
  "funding": {
    "type": "paypal",
    "url": "https://www.paypal.com/donate/?business=UM6EEKPW8GXA2&no_recurring=0&item_name=Open+source+development&currency_code=USD"
  },
  "repository": "github:nicfv/npm",
  "homepage": "https://npm.nicfv.com/viridis",
  "license": "MIT",
  "dependencies": {
    "smath": "1.1.1"
  },
  "devDependencies": {
    "typedoc": "0.25.11",
    "typescript": "5.3.3"
  }
}