    "clean": "rm -rf node_modules package-lock.json dist types docs",
    "docs": "rm -rf docs && typedoc --includeVersion --disableSources --hideGenerator src",
    "prepack": "npm run build",
    "postpack": "rm -rf dist types"
  },
