{
  "name": "viridis",
  "version": "1.0.13",
  "description": "Color gradients for data visualization",
  "homepage": "https://npm.nicfv.com/viridis",
  "bin": "",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rm -rf dist types && tsc && node dist/test.js && rm dist/test.js",
