{
  "name": "exray",
  "version": "1.1.2",
  "description": "Lightweight assertion testing framework",
  "homepage": "https://npm.nicfv.com/exray",
  "bin": "",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "npm run postpack && typedoc && node dist/test.js && rm dist/test.js types/test.d.ts",
    "test": "tsc -v && tsc --noEmit",
    "clean": "rm -rf node_modules package-lock.json && npm run postpack",
    "prepack": "npm run build",
    "postpack": "rm -rf dist types docs"
  },
  "keywords": [
    "except",
    "exception",
    "test",
    "testing",
    "framework",
    "assert",
    "assertion",
    "pass",
    "fail"
  ],
